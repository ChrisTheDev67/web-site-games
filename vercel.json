{
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.zip",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "Content-Type",
                    "value": "application/zip"
                }
            ]
        }
    ]
}