{
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.apk",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate, max-age=0"
                },
                {
                    "key": "Content-Type",
                    "value": "application/octet-stream"
                }
            ]
        }
    ]
}